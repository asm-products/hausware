<% content_for :page_title do %>
ordered_good Details - 
<% end %>
<div class="container">
<div class="page-header">
<h1>
<div class="pull-left">
<%= link_to "<span class=\"glyphicon glyphicon-step-backward\" aria-hidden=\"true\"></span>".html_safe, ordered_goods_path, class: "btn btn-default", title: "Back to ordered_goods" %>
&nbsp;
</div>
<div class="pull-right">
<%= link_to "<span class=\"glyphicon glyphicon-pencil\" aria-hidden=\"true\"></span> Edit".html_safe, edit_ordered_good_path(@ordered_good), class: "btn btn-warning" %>
</div>
ordered_good Details
</h1>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Order</label>
  <div class="col-sm-9"><%= @ordered_good.order %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Good</label>
  <div class="col-sm-9"><%= @ordered_good.good %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Name</label>
  <div class="col-sm-9"><%= @ordered_good.name %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Quantity</label>
  <div class="col-sm-9"><%= @ordered_good.quantity %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Subtotal in cents</label>
  <div class="col-sm-9"><%= @ordered_good.subtotal_in_cents %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Fulfilled at</label>
  <div class="col-sm-9"><%= @ordered_good.fulfilled_at %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Canceled at</label>
  <div class="col-sm-9"><%= @ordered_good.canceled_at %></div>
</div>

</div>

