<% content_for :page_title do %>
Edit Ordered Good - 
<% end %>
<div class="container">
<div class="page-header">
<h1>Editing Ordered Good</h1>
</div>

<%= render 'form' %>

<div class="clearfix"></div>
<div class="pull-left">
<%= link_to "<span class=\"glyphicon glyphicon-ban-circle\" aria-hidden=\"true\"></span> Cancel".html_safe, @ordered_good, class: "btn btn-default" %>
</div>
<div class="pull-right">
<%= link_to "<span class=\"glyphicon glyphicon-trash\" aria-hidden=\"true\"></span> Delete ordered_good...".html_safe, @ordered_good, method: :delete, data: { confirm: "Are you sure you want to delete this ordered_good?" }, class: "btn btn-danger" %>
</div>
</div>
